Algoritmo "Lista01Ex03"

Var
   altura, volume, raio: real

Inicio
      escreva ("Digite a Altura:")
      leia(altura)
      
      escreva ("Digite o raio:")
      leia(raio)
      
      
      volume <- 3.14 * (raio * raio) * altura
      
      escreva (" O volume da lata é: ",volume)


Fimalgoritmo