Algoritmo "Lista01Ex04"

Var
     n1, n2, n3: real

Inicio
      escreva ("Digite o primeiro número:")
      leia(n1)
      
      escreva ("Digite o segundo número:")
      leia(n2)

      n3 <- 3
      
      n3 <- n1
      n1 <- n2
      n2 <- n3
      
      escreva(" O valor do primeiro número é: ",n1)
      escreva(" O valor do segundo número é: ",n2)


Fimalgoritmo